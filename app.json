{
  "expo": {
    "name": "AIdWell",
    "slug": "aidwell",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://picsum.photos/200",
    "scheme": "aidwell",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "https://picsum.photos/1080/1920",
      "resizeMode": "contain",
      "backgroundColor": "#e1f16b"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aidwell.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://picsum.photos/200",
        "backgroundColor": "#e1f16b"
      },
      "package": "com.aidwell.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "https://picsum.photos/64"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}